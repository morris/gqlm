{
  "name": "gqlm",
  "version": "0.1.1",
  "description": "A framework for fully automatic monkey testing of GraphQL APIs",
  "keywords": [
    "graphql",
    "test",
    "testing",
    "monkey",
    "automation"
  ],
  "homepage": "https://github.com/morris/gqlm",
  "repository": {
    "type": "git",
    "url": "https://github.com/morris/gqlm.git"
  },
  "bugs": {
    "url": "https://github.com/morris/gqlm/issues"
  },
  "author": "Morris Brodersen <mb@morrisbrodersen.de> (http://morrisbrodersen.de)",
  "license": "ISC",
  "engines": {
    "node": ">=14"
  },
  "module": "src/index.ts",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist && tsc --project tsconfig.build.json",
    "clean": "rm -rf coverage dist __gqlm__",
    "lint": "eslint .",
    "format": "prettier --write .",
    "format-check": "prettier --check .",
    "test": "jest --runInBand --coverage",
    "maybe-publish": "bash scripts/maybe-publish.sh"
  },
  "dependencies": {
    "chance": "^1.1.11",
    "graphql": "^16.6.0",
    "node-fetch": "^2.6.9",
    "pluralize": "^8.0.0",
    "prettier": "^2.8.7"
  },
  "devDependencies": {
    "@types/chance": "^1.1.3",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/node-fetch": "^2.6.2",
    "@types/pluralize": "0.0.29",
    "@types/prettier": "^2.4.2",
    "@types/ws": "^8.5.4",
    "@typescript-eslint/eslint-plugin": "^5.6.0",
    "@typescript-eslint/parser": "^5.6.0",
    "body-parser": "^1.20.2",
    "eslint": "^8.4.1",
    "express": "^4.18.2",
    "graphqlade": "^1.5.1",
    "jest": "^29.5.0",
    "ts-jest": "^29.0.5",
    "ts-node": "^10.9.1",
    "typescript": "^4.9.5"
  }
}
